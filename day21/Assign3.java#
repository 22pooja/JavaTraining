//3.Create an ArrayList which will be able to store only numbers like int,float,double,etc, but not any 
//other data type.

import java .util.*;

class Assign3
{
public static void main(String []args) 
{
ArrayList<Integer>ar;
ar=new ArrayList<>();
ar.add(10);
ar.add(20);
ar.add(20.2);
// ar.add();
// ar.add("E");

Iterator<String> it1;
it1=ar.iterator();
 while(it1.hasNext())
 {
  System.out.println(it1.next());
        }
}
}

